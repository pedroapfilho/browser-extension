{
  "manifest_version": 2,
  "name": "Browser Extension",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "description": "Something here",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Browser Extension"
  },
  "permissions": ["tabs", "<all_urls>", "storage"]
}
